{
    "name": "jasny/dotkey",
    "description": "Dot notation access for objects and arrays",
    "keywords": ["json", "object", "array", "query"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
    ],
    "support": {
        "issues": "https://github.com/jasny/dotkey/issues",
        "source": "https://github.com/jasny/dotkey"
    },
    "require": {
        "php": ">=7.4.0"
    },
    "require-dev": {
        "jasny/php-code-quality": "2.6.*"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\DotKey\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jasny\\DotKey\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpstan analyse",
            "phpunit --testdox --colors=always",
            "phpcs -p src"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
